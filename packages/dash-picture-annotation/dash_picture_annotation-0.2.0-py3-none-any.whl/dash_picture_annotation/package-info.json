{
  "name": "dash_picture_annotation",
  "packageManager": "yarn@4.7.0",
  "version": "0.2.0",
  "description": "Dash porting version of the React project React Picture Annotation. Provide a simple annotation window for a single picture.",
  "repository": {
    "type": "git",
    "url": "git://github.com/cainmagi/dash-picture-annotation.git"
  },
  "bugs": {
    "url": "https://github.com/cainmagi/dash-picture-annotation/issues"
  },
  "homepage": "https://github.com/cainmagi/dash-picture-annotation",
  "keywords": [
    "python",
    "python3",
    "python-library",
    "dash",
    "plotly-dash",
    "python-dash",
    "annotation",
    "annotation-tool",
    "image-annotation",
    "labeling",
    "image-labeling",
    "object-detection",
    "deep-learning",
    "data-visualization"
  ],
  "main": "build/index.js",
  "scripts": {
    "start": "webpack serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components dash_picture_annotation -p package-info.json --r-prefix 'dpa' --jl-prefix 'dpa' --ignore \\.test\\.",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:backends)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Yuchen Jin (cainmagi) <cainmagi@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@plotly/dash-component-plugins": "^1.2.3",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "clsx": "^2.1.1",
    "colord": "^2.9.3",
    "copyfiles": "^2.1.1",
    "css-loader": "^6.8.1",
    "disable-scroll": "^0.6.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "prop-types": "^15.8.1",
    "ramda": "^0.26.1",
    "react": "~18.2.0",
    "react-docgen": "^5.4.3",
    "react-dom": "~18.2.0",
    "react-image-size": "^2.4.0",
    "react-picture-annotation": "~1.2.0",
    "react-select": "^5.8.0",
    "react-use": "^17.5.1",
    "style-loader": "^3.3.3",
    "styled-jsx": "^4.0.1",
    "use-lru-cache": "^1.1.5",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
